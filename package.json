{
  "name": "collman",
  "version": "0.0.2",
  "description": "collection manager",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "type-check": "tsc",
    "type-check:watch": "tsc -w",
    "docsify:serve": "docsify serve /Users/ramonmorcillo/Documents/developer/collection-manager/src/test/mocks/defaultCollection/output/markdown/defaultCollectionWithDocsify/docs"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm run test && npm run type-check && npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reymon359/collman.git"
  },
  "keywords": [
    "collection",
    "manager"
  ],
  "author": "reymon359",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reymon359/collection-manager/issues"
  },
  "homepage": "https://github.com/reymon359/collection-manager#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.8",
    "jdown": "^1.0.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "docsify-cli": "^4.4.2",
    "fs-extra": "^9.1.0",
    "json2md": "^1.9.2"
  }
}
